<style type='text/css'>
  body{ font-family: Arial, Helvetica, sans-serif; font-weight: 200; }
  table{width: 100%;} table tr td{ border: 1px solid #000; padding: 8px; }
</style>
<html>
  <body>
    <h1>Vaccine History</h1>
    <p>{{id.name.first}} {{id.name.last}}</p>
    <p>{{id.animal}} {{id.color}} {{id.breed}}</p>
    <table>
      <tr>
        <th>
          Type
        </th>
        <th>
          date
        </th>
      </tr>
      {{#each shots as |shot|}}
        <tr>
          <td>
            {{shot.type}}
          </td>
          <td>
            {{dateFormat shot.date 'MMM Do'}}
          </td>
        </tr>
      {{/each}}
    </table>
  </body>
</html>